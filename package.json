{
  "name": "aws-simple",
  "version": "2.2.1",
  "description": "A Node.js interface for AWS that allows easy configuration and deployment of simple web apps.",
  "keywords": [
    "apigateway",
    "aws",
    "cdk",
    "cloudformation",
    "deploy",
    "deployment",
    "lambda",
    "s3",
    "serverless"
  ],
  "bugs": {
    "url": "https://github.com/clebert/aws-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/aws-simple.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "types": "lib/types.d.ts",
  "bin": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "compile": "tsc --build",
    "postcompile": "chmod +x lib/index.js",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "lint": "tslint --config tslint.json --project tsconfig.json --format verbose",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "prestart": "yarn compile",
    "start": "node ./lib/index.js start",
    "test": "jest",
    "preupdate-readme": "yarn compile",
    "update-readme": "node update-readme.js"
  },
  "dependencies": {
    "@aws-cdk/assets": "^1.31.0",
    "@aws-cdk/aws-apigateway": "^1.31.0",
    "@aws-cdk/aws-certificatemanager": "^1.31.0",
    "@aws-cdk/aws-cloudformation": "^1.31.0",
    "@aws-cdk/aws-cloudfront": "^1.31.0",
    "@aws-cdk/aws-cloudwatch": "^1.31.0",
    "@aws-cdk/aws-ec2": "^1.31.0",
    "@aws-cdk/aws-elasticloadbalancing": "^1.31.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.31.0",
    "@aws-cdk/aws-events": "^1.31.0",
    "@aws-cdk/aws-iam": "^1.31.0",
    "@aws-cdk/aws-kms": "^1.31.0",
    "@aws-cdk/aws-lambda": "^1.31.0",
    "@aws-cdk/aws-logs": "^1.31.0",
    "@aws-cdk/aws-route53": "^1.31.0",
    "@aws-cdk/aws-route53-targets": "^1.31.0",
    "@aws-cdk/aws-s3": "^1.31.0",
    "@aws-cdk/aws-s3-assets": "^1.31.0",
    "@aws-cdk/aws-sns": "^1.31.0",
    "@aws-cdk/aws-sqs": "^1.31.0",
    "@aws-cdk/aws-ssm": "^1.31.0",
    "@aws-cdk/cloud-assembly-schema": "1.33.1",
    "@aws-cdk/core": "^1.31.0",
    "@aws-cdk/cx-api": "^1.31.0",
    "@aws-cdk/region-info": "^1.31.0",
    "aws-sdk": "^2.647.0",
    "chalk": "^4.0.0",
    "chokidar": "^3.3.1",
    "cliui": "^6.0.0",
    "compose-function": "^3.0.3",
    "compression": "^1.7.4",
    "constructs": "^3.0.0",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "ink": "^2.7.1",
    "ink-spinner": "^3.0.1",
    "lambda-local": "^1.7.1",
    "listr": "^0.14.3",
    "mime-types": "^2.1.26",
    "prompts": "^2.3.2",
    "react": "^16.13.1",
    "signal-exit": "^3.0.3",
    "source-map-support": "^0.5.16",
    "url-join": "^4.0.1",
    "winston": "^3.2.1",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/aws-lambda": "^8.10.46",
    "@types/compose-function": "^0.0.30",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.3",
    "@types/jest": "^25.1.4",
    "@types/listr": "^0.14.2",
    "@types/mime-types": "^2.1.0",
    "@types/prompts": "^2.0.5",
    "@types/react": "^16.9.26",
    "@types/signal-exit": "^3.0.0",
    "@types/url-join": "^4.0.0",
    "@types/yargs": "^15.0.4",
    "aws-cdk": "^1.31.0",
    "babel-jest": "^25.2.3",
    "ink-testing-library": "^1.0.3",
    "jest": "^25.2.3",
    "prettier": "^2.0.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "aws-cdk": "^1.31.0"
  }
}
