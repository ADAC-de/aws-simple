{
  "name": "aws-simple",
  "version": "9.4.2",
  "description": "A Node.js interface for AWS that allows easy configuration and deployment of simple web apps.",
  "bugs": {
    "url": "https://github.com/clebert/aws-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/aws-simple.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "types": "lib/cjs/types.d.ts",
  "bin": {
    "aws-simple": "lib/cjs/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "ci": "run-p format:check compile lint test update-readme",
    "postci": "chmod +x lib/cjs/index.js",
    "compile": "onecmd compile",
    "format": "onecmd format",
    "format:check": "onecmd format --check",
    "lint": "onecmd lint",
    "prepare": "run-s setup",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "setup": "onecmd setup",
    "prestart": "npm run compile",
    "start": "node ./lib/cjs/index.js start",
    "test": "onecmd test",
    "preupdate-readme": "npm run compile",
    "update-readme": "node update-readme.js"
  },
  "dependencies": {
    "@rtmpl/terminal": "^3.0.0",
    "aws-cdk-lib": "^2.0.0-rc.21",
    "aws-sdk": "^2.984.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "cli-spinners": "^2.6.1",
    "cliui": "^7.0.4",
    "compose-function": "^3.0.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "ink": "^3.2.0",
    "ink-spinner": "^4.0.3",
    "lambda-local": "^2.0.0",
    "listr": "^0.14.3",
    "mime-types": "^2.1.32",
    "mkdirp": "^1.0.4",
    "p-retry": "^4.6.1",
    "prompts": "^2.4.1",
    "react": "^17.0.2",
    "rtmpl": "^1.3.0",
    "signal-exit": "^3.0.3",
    "source-map-support": "^0.5.19",
    "url-join": "^4.0.1",
    "winston": "^3.3.3",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@onecmd/standard-plugins": "^0.13.1",
    "@types/aws-lambda": "^8.10.85",
    "@types/compose-function": "^0.0.30",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/listr": "^0.14.4",
    "@types/mime-types": "^2.1.1",
    "@types/mkdirp": "^1.0.2",
    "@types/prompts": "^2.0.14",
    "@types/react": "^17.0.37",
    "@types/signal-exit": "^3.0.1",
    "@types/url-join": "^4.0.1",
    "@types/yargs": "^17.0.7",
    "ink-testing-library": "^2.1.0",
    "npm-run-all": "^4.1.5"
  }
}
