{
  "name": "aws-simple",
  "version": "0.0.3",
  "description": "A Node.js interface for AWS that allows easy configuration and deployment of simple web projects.",
  "keywords": [
    "apigateway",
    "aws",
    "cdk",
    "cloudformation",
    "deploy",
    "deployment",
    "lambda",
    "s3",
    "serverless"
  ],
  "bugs": {
    "url": "https://github.com/clebert/aws-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/aws-simple.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "bin": "lib/index.js",
  "scripts": {
    "precompile": "rm -rf lib/",
    "compile": "tsc",
    "format": "prettier --write '**/*.{js,json,md,ts,yml}'",
    "lint": "tslint --project . --format stylish",
    "prestart": "run-s compile",
    "start": "node ./lib/index.js start"
  },
  "dependencies": {
