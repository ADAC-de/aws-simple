{
  "name": "aws-simple",
  "version": "0.0.13",
  "description": "A Node.js interface for AWS that allows easy configuration and deployment of simple web apps.",
  "keywords": [
    "apigateway",
    "aws",
    "cdk",
    "cloudformation",
    "deploy",
    "deployment",
    "lambda",
    "s3",
    "serverless"
  ],
  "bugs": {
    "url": "https://github.com/clebert/aws-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/aws-simple.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "bin": "lib/index.js",
  "scripts": {
    "precompile": "rm -rf lib/",
    "compile": "tsc",
    "postcompile": "chmod +x lib/index.js",
    "format": "prettier --write '**/*.{js,json,md,ts,yml}'",
    "lint": "tslint --project . --format stylish",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "prestart": "run-s compile",
    "start": "node ./lib/index.js start",
    "test": "jest --silent --verbose"
  },
  "dependencies": {
    "@aws-cdk/assets": "^1.12.0",
    "@aws-cdk/aws-apigateway": "^1.12.0",
    "@aws-cdk/aws-certificatemanager": "^1.12.0",
    "@aws-cdk/aws-cloudformation": "^1.12.0",
    "@aws-cdk/aws-cloudfront": "^1.12.0",
    "@aws-cdk/aws-cloudwatch": "^1.12.0",
    "@aws-cdk/aws-ec2": "^1.12.0",
    "@aws-cdk/aws-elasticloadbalancing": "^1.12.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.12.0",
    "@aws-cdk/aws-events": "^1.12.0",
    "@aws-cdk/aws-iam": "^1.12.0",
    "@aws-cdk/aws-kms": "^1.12.0",
    "@aws-cdk/aws-lambda": "^1.12.0",
    "@aws-cdk/aws-logs": "^1.12.0",
    "@aws-cdk/aws-route53": "^1.12.0",
    "@aws-cdk/aws-route53-targets": "^1.12.0",
    "@aws-cdk/aws-s3": "^1.12.0",
    "@aws-cdk/aws-s3-assets": "^1.12.0",
    "@aws-cdk/aws-sns": "^1.12.0",
    "@aws-cdk/aws-sqs": "^1.12.0",
    "@aws-cdk/aws-ssm": "^1.12.0",
    "@aws-cdk/core": "^1.12.0",
    "@aws-cdk/cx-api": "^1.12.0",
    "@aws-cdk/region-info": "^1.12.0",
    "aws-sdk": "^2.547.0",
    "chalk": "^2.4.2",
    "chokidar": "^3.2.1",
    "cliui": "^5.0.0",
    "compose-function": "^3.0.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "get-port": "^5.0.0",
    "lambda-local": "^1.6.3",
    "listr": "^0.14.3",
    "mime-types": "^2.1.24",
    "prettier": "^1.18.2",
    "prompts": "^2.2.1",
    "source-map-support": "^0.5.13",
    "url-join": "^4.0.1",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/aws-lambda": "^8.10.33",
    "@types/compose-function": "^0.0.30",
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/listr": "^0.14.2",
    "@types/mime-types": "^2.1.0",
    "@types/prompts": "^2.0.1",
    "@types/url-join": "^4.0.0",
    "@types/yargs": "^13.0.3",
    "aws-cdk": "^1.12.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "ts-config": "^20.10.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "peerDependencies": {
    "aws-cdk": "^1.12.0"
  }
}
