{
  "name": "aws-simple",
  "version": "6.2.0",
  "description": "A Node.js interface for AWS that allows easy configuration and deployment of simple web apps.",
  "keywords": [
    "apigateway",
    "aws",
    "cdk",
    "cloudformation",
    "deploy",
    "deployment",
    "lambda",
    "s3",
    "serverless"
  ],
  "bugs": {
    "url": "https://github.com/clebert/aws-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/aws-simple.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "types": "lib/types.d.ts",
  "bin": {
    "aws-simple": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.json --incremental --pretty",
    "postbuild": "chmod +x lib/index.js",
    "ci": "run-p format:test update-readme lint test",
    "format": "prettier --write '**/*.{cjs,html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{cjs,html,js,json,md,ts,tsx,yml}'",
    "lint": "tslint --config tslint.json --project tsconfig.json --format verbose",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "prestart": "npm run build",
    "start": "node ./lib/index.js start",
    "test": "jest",
    "preupdate-readme": "npm run build",
    "update-readme": "node update-readme.js"
  },
  "dependencies": {
    "aws-cdk-lib": "next",
    "aws-sdk": "^2.956.0",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.2",
    "cliui": "^7.0.4",
    "compose-function": "^3.0.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "ink": "^3.0.9",
    "ink-spinner": "^4.0.2",
    "lambda-local": "^2.0.0",
    "listr": "^0.14.3",
    "mime-types": "^2.1.32",
    "mkdirp": "^1.0.4",
    "p-retry": "^4.6.1",
    "prompts": "^2.4.1",
    "react": "^16.14.0",
    "signal-exit": "^3.0.3",
    "source-map-support": "^0.5.19",
    "url-join": "^4.0.1",
    "winston": "^3.3.3",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/aws-lambda": "^8.10.81",
    "@types/cli-spinners": "^1.3.0",
    "@types/compose-function": "^0.0.30",
    "@types/compression": "^1.7.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/listr": "^0.14.4",
    "@types/mime-types": "^2.1.0",
    "@types/mkdirp": "^1.0.2",
    "@types/prompts": "^2.0.14",
    "@types/react": "^17.0.15",
    "@types/signal-exit": "^3.0.1",
    "@types/url-join": "^4.0.1",
    "@types/yargs": "^17.0.2",
    "babel-jest": "^27.0.6",
    "ink-testing-library": "^2.1.0",
    "jest": "^27.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.3.2",
    "tslint": "^6.1.3",
    "typescript": "~4.3.5"
  }
}
