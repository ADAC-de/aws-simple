    "@types/listr": "^0.14.2",
    "@types/mime-types": "^2.1.0",
    "@types/url-join": "^4.0.0",
    "@types/yargs": "^13.0.2",
    "npm-run-all": "^4.1.5",
    "ts-config": "^20.10.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  }
