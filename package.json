{
  "name": "aws-simple",
  "version": "10.0.3",
  "description": "A Node.js interface for AWS that allows easy configuration and deployment of simple web apps.",
  "bugs": {
    "url": "https://github.com/clebert/aws-simple/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/aws-simple.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "types": "lib/index.d.ts",
  "bin": {
    "aws-simple": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "ci": "run-p format:check compile lint test",
    "compile": "tsc --project tsconfig.json --incremental --pretty",
    "postcompile": "chmod +x lib/index.js",
    "format": "onecmd format",
    "format:check": "onecmd format --check",
    "lint": "onecmd lint",
    "prepare": "run-s setup",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "setup": "onecmd setup",
    "start": "node ./lib/index.js",
    "test": "onecmd test"
  },
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.50.0",
    "@aws-sdk/client-cloudformation": "^3.50.0",
    "@aws-sdk/client-s3": "^3.50.0",
    "aws-cdk-lib": "^2.12.0",
    "chalk": "^4.1.2",
    "lambda-local": "^2.0.1",
    "mime-types": "^2.1.34",
    "prompts": "^2.4.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@onecmd/standard-plugins": "^0.18.0",
    "@types/aws-lambda": "^8.10.92",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.17",
    "@types/prompts": "^2.0.14",
    "@types/yargs": "^17.0.8",
    "npm-run-all": "^4.1.5"
  }
}
